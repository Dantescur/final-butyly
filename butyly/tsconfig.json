{
	"compilerOptions": {
		"target": "ES2020",
		"baseUrl": "./",
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"paths": {
			"@services/*": ["src/services/*"],
			"@utils/*": ["src/utils/*"]
		},
		"outDir": "./dist",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true
	}
}
